      "prefix": ["px", "egg", "eggjs", "ec"],
      "body": [
        "const Controller = require('egg').Controller",
        "",
        "class ${TM_FILENAME_BASE/([a-zA-Z0-9])([a-zA-Z0-9]+)/${1:/upcase}${2}/g} extends Controller {",
